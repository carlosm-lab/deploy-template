{
    "version": 2,
    "builds": [
        {
            "src": "api/index.py",
            "use": "@vercel/python"
        },
        {
            "src": "static/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/favicon.ico",
            "dest": "/static/favicon.svg"
        },
        {
            "src": "/favicon.svg",
            "dest": "/static/favicon.svg"
        },
        {
            "src": "/manifest.json",
            "dest": "/static/manifest.json"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.py"
        }
    ]
}